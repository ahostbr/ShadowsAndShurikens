{
  "pack_name": "trace_basics",
  "version": "0.1.0",
  "description": "BPGen snippet pack for simple KismetSystemLibrary traces (Line and Sphere).",
  "author": "Ryan",
  "snippets": [
    {
      "snippet_name": "Trace_LineSingle",
      "description": "Performs a single line trace and returns Hit and Success.",
      "job_file": "jobs/Trace_LineSingle_Job.json",
      "graphspec_file": "graphspecs/Trace_LineSingle_GraphSpec.json",
      "node_classes": [
        "K2Node_FunctionEntry",
        "K2Node_CallFunction",
        "K2Node_FunctionResult"
      ]
    },
    {
      "snippet_name": "Trace_LineMulti",
      "description": "Performs a multi line trace, returning an array of HitResults.",
      "job_file": "jobs/Trace_LineMulti_Job.json",
      "graphspec_file": "graphspecs/Trace_LineMulti_GraphSpec.json",
      "node_classes": [
        "K2Node_FunctionEntry",
        "K2Node_CallFunction",
        "K2Node_FunctionResult"
      ]
    },
    {
      "snippet_name": "Trace_SphereSingle",
      "description": "Performs a single sphere trace and returns Hit and Success.",
      "job_file": "jobs/Trace_SphereSingle_Job.json",
      "graphspec_file": "graphspecs/Trace_SphereSingle_GraphSpec.json",
      "node_classes": [
        "K2Node_FunctionEntry",
        "K2Node_CallFunction",
        "K2Node_FunctionResult"
      ]
    }
  ]
}